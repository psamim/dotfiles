{
  "window.commandCenter": false,
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.codeActionsOnSave.rules": null,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "l", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "l", "r"],
      "commands": ["editor.action.rename"],
      "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["git.showGitStatus"]
    },
    {
      "before": ["<leader>", "g", "t"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "b", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.renderSideBySide": false,
  "terminal.integrated.scrollback": 100000,
  "editor.formatOnSave": true,
  "vim.digraphs": {},
  "workbench.settings.openDefaultKeybindings": true,
  "vim.commandLineModeKeyBindingsNonRecursive": [],
  "redhat.telemetry.enabled": true,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.enableNeovim": true,
  "window.zoomLevel": 2,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "notebook.lineNumbers": "on",
  "editor.fontFamily": "Iosevka Etoile, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "gruvboxMaterial.colorfulSyntax": true,
  "gruvboxMaterial.darkWorkbench": "flat",
  "editor.minimap.enabled": false,
  "terminal.integrated.fontFamily": "Iosevka Nerd Font Mono",
  "gitlab.duo.enabledWithoutGitlabProject": false,
  "gitlab.duoChat.enabled": false,
  "gitlab.duoCodeSuggestions.openTabsContext": false,
  "gitlab.duoCodeSuggestions.enabled": false,
  "jestrunner.runOptions": ["--no-coverage"],
  "typescript.preferences.importModuleSpecifier": "relative",
  "javascript.preferences.importModuleSpecifier": "relative",
  "jestrunner.debugOptions": {},
  "javascript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "cursor.terminal.usePreviewBox": true,
  "php.validate.executablePath": "/opt/homebrew/opt/php@8.2/bin/php",
  "git.autorefresh": false
}
